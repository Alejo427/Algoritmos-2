// variables
import java.util.Scanner;

public class FizzBuzz {
    
    public static void main(String[] args){
            int numero;
// Scanner se usa para leer
        Scanner leeScanner=new Scanner(System.in);
            System.out.println("INGRESE UN NUMERO HASTA EL 100");
            numero=leeScanner.nextInt();
        // i PARA GUARDAR LA VARIABLE
        int i;
            // for PARA EL BUCLE DE 100 NUMEROS
        for(i=0;i<100;i++){
                // CONDICIONAL IF ELSE
                if(i%3 == 0 && i%5 == 0){
                    System.out.println("FizzBuzz");
                }
                else if(i%3 == 0){
                    System.out.println("Fizz");
                }
                else if(i%5 == 0){
                    System.out.println("Buzz");
                }
                else {
										System.out.println(i);
                }
                }
                
                
            }
        }
