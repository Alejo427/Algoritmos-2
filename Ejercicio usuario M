// SCANNER 
import java.util.Scanner;
import java.util.ArrayList;

public class EjercicioTparcial {

	//funcion principal
public static void main(String[] args) {
	Scanner LeerScanner = new Scanner(System.in);
	
	System.out.println("INGRESE EL TAMAÃ‘O DEL ARREGLO");
	int N = LeerScanner.nextInt();
	
	// declarar el arreglo y despues inicializarlo
	
	int Arreglo [] = new int [N];
	
	//num arreglos
	
	System.out.println("INGRESE LOS NUMEROS DEL ARREGLO");
	
	for(int i= 0; i<N; i++) {
		System.out.println("NUMERO"+(i+1)+": ");
			Arreglo[i] = LeerScanner.nextInt();
		
		}
	
	System.out.println("INGRESE UN NUMERO ENTERO");
	int M = LeerScanner.nextInt();
	
	
	}
}
